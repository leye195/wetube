{
  "name": "wetube",
  "version": "1.0.0",
  "description": "YouTube Clone Coding",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss' --ignore 'static'",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/static','src/assets','src/webpack.config.js' ",
    "tunnel": "ngrok http 8080",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "author": "YoungJae",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.8.3",
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "aws-sdk": "^2.610.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.0",
    "helmet": "^3.21.2",
    "mongoose": "^5.8.7",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "ngrok": "^3.2.7",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-kakao": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "passport-naver": "^1.0.6",
    "pug": "^2.0.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3"
  }
}
